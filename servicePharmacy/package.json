  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev:server": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "node dist/server.js",
    "typeorm": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "proto:build": "rm -rf src/protos/generated && mkdir src/protos/generated && pbjs -t static-module -w commonjs -o src/protos/generated/index.js src/protos/pb/*.proto && pbts -o src/protos/generated/index.d.ts src/protos/generated/index.js && echo \"Generated files! ðŸš€\"",
    "test": "jest"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.3",
    "dotenv": "^8.2.0",
    "google-protobuf": "^3.11.4",
    "grpc": "^1.24.11",
    "mariadb": "^2.5.5",
    "mysql2": "^2.3.2",
    "protobufjs": "^6.8.9",
    "protoc-gen-grpc": "^1.4.0",
    "reflect-metadata": "^0.1.10",
    "tsyringe": "^4.6.0",
    "typeorm": "0.2.31"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
